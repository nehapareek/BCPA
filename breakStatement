// I think there is a bug because even after changing value of j and i, I am getting the same result.
#include <stdio.h>
int main(){
	int i,j;
	 for (i=1; i<5; i++){
		printf("outer for loop, i =%d\n",i);
		for (j=1; j<5; j++){
			if(j==4){
				printf("Breaking out from the loop\n");
				break;
			}
			printf("inner for loop, j=%d\n",j);
		}
		printf("\n");
	}
	}
