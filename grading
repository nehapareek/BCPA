//grading 
#include <stdio.h>
int main(){
	int a;
	printf("enter the marks obtained:");
	scanf("%d",&a);
	if (100>=a && a>=90){
	printf("grade is A");}
	else if (89>=a && a>=80){
	printf("grade is B");}
	else if (79>=a && a>=70){
	printf("grade is C");}
	else if (69>=a && a>=50){
	printf("grade is D");}
	else if (49>=a && a>=35){
	printf("grade is E");}
	else{
	printf("grade is F");}
}
